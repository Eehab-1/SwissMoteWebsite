@model IEnumerable<SwissMoteWebsite.Models.ImageProof>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/FigmaMaster.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
