            @Html.DisplayFor(modelItem => item.MouseClicks)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DesktopUniqueSession)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DesktopTimer)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
